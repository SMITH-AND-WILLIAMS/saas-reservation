  "description": "This project is the base for all other projects made in ode where we wish to use ES6 full syntax. It will be transpiled with babel",
  "main": "src/index.js",
  "scripts": {
    "lint": "./node_modules/.bin/eslint src/** || exit 0",
    "build": "./node_modules/.bin/babel src -d dist",
    "prestart": "npm run build",
    "start": "node dist/",
    "dev": "./node_modules/.bin/nodemon src/",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SMITH-AND-WILLIAMS/node-ES6-base.git"
  },
  "keywords": [
