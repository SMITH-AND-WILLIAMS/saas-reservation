{
  "name": "node-es6-base",
  "version": "0.0.1",
  "description": "This project is the base for all other projects made in ode where we wish to use ES6 full syntax. It will be transpiled with babel",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src/**",
    "build": "babel src -d dist",
    "prepublish": "npm run build",
    "start": "node lib/",
    "dev": ". / node_modules / .bin / babel - node src / ",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SMITH-AND-WILLIAMS/node-ES6-base.git"
  },
  "keywords": [
    "NODE",
    "ES6",
    "SMITH&WILLIAMS",
    "ESLINT",
    "BABEL",
    "NPM"
  ],
  "author": "The Crazy Digital Painter",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SMITH-AND-WILLIAMS/node-ES6-base/issues"
  },
  "homepage": "https://github.com/SMITH-AND-WILLIAMS/node-ES6-base#readme",
  "dependencies": {
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-preset-env": "^1.1.8",
    "babel-preset-es2015": "^6.22.0",
    "eslint": "^3.14.1",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-plugin-import": "^2.2.0"
  }
